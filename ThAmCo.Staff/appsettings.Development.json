// Example layout. Actual configuration would be inside of secrets
{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Services": {
    "Products": {
      "BaseAddress": "https://localhost:7068/",
      "UseFake": false,
      "AuthAudience": "https://<YOUR_API_IDENTIFIER>"
    }
  },
  "Auth": {
    "Domain": "<YOUR_AUTH0_TENANT>.eu.auth0.com",
    "ClientId": "<YOUR_AUTH0_CLIENT_ID>",
    "Audience": "https://<YOUR_API_IDENTIFIER>"
  }
}
